{
  "name": "@howtocards/frontend",
  "version": "0.4.0",
  "description": "Save and share your solutions",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "yarn test:lint && yarn test:code",
    "test:code": "node scripts/test.js",
    "test:lint": "eslint .",
    "commit": "git-cz",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "homepage": "https://test.cards.atomix.team",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test:lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,eslintrc}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howtocards/frontend.git"
  },
  "author": "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howtocards/frontend/issues"
  },
